{
  "compilerOptions": {
      "lib": [
          "dom",
          "dom.iterable",
          "esnext"
      ],
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "jsx": "react"
  },
  "include": [
      "src",
      "src/react-app-env.d.ts",
      "src/api.ts",
      "public/assets",
      "custom.d.ts"
  ]
}
